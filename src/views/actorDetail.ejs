<%- include("partials/head") %> 
<body>
    <%- include("partials/header") %> 
    <div style="margin: 10px; display: flex; flex-direction: column;">
       <h1>Nombre de actor: <%= actor.first_name %> <%= actor.last_name %></h1>
        <h3>Rating: <%= actor.rating %> </h3>
        <h3>Peliculas donde actuo:</h3>
            <ul>
                <% actor.movies.forEach(movie => { %>
                    <li><a href="/movies/detail/<%= movie.id %>"><%= movie.title %></a></li>
                <% }) %>
            </ul>
        
    </div>
</body>
</html>